10 DIM IDADES[20]
20 CLS
25 I = 1
30 INPUT "DIGITE UMA IDADE (0 PARA TERMINAR)", IDADES[I]
32 IF IDADES[I] = 0 THEN GOTO 80
33 I = I + 1
40 GOTO 30
50 GOSUB 70
55 PRINT "FORAM COLETADAS ",I," IDADES"
60 PRINT "A IDADE MEDIA E: ".MEDIA
65 END
70 ACUM = 0
80 FOR J = 1 TO I
90 ACUM = ACUM + IDADES[I]
95 NEXT I
100 MEDIA = ACUM / I
105 RETURN
